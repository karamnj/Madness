<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sirius.madness.ui.activities.DiscoverActivity"
    tools:ignore="MergeRootFrame"
    android:background="@color/theme_neutral_light_grey">

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scheduleList"
        android:divider="@color/theme_neutral_light_grey"
        android:dividerHeight="5dp"
        android:paddingBottom="80dp"
        android:clipToPadding="false"></ListView>

    <include layout="@layout/menu_layout_with_bar" />
    <include
        layout="@layout/menu_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|bottom"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="16dp" />

</FrameLayout>
